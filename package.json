    "fs-extra": "9.0.0",
    "jsdom": "16.2.1",
    "mocha": "7.1.1",
    "mocha-junit-reporter": "1.23.3",
    "mocha-parallel-tests": "2.3.0",
    "nock": "12.0.3",
    "nyc": "15.0.0",

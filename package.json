  },
  "homepage": "https://github.com/adobe/helix-pages#readme",
  "dependencies": {
    "@adobe/helix-pipeline": "6.9.3",
    "@adobe/helix-shared": "6.0.5",
    "@adobe/openwhisk-action-logger": "2.2.0",
    "@adobe/openwhisk-action-utils": "4.2.2",
    "algoliasearch": "4.1.0",
    "ferrum": "1.7.0",
    "jquery": "3.4.1",

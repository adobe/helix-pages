    "jquery": "3.4.1",
    "request": "2.88.0",
    "request-promise-native": "1.0.8"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.0",
    "@semantic-release/changelog": "3.0.5",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/github": "5.5.5",
    "chai": "4.2.0",
